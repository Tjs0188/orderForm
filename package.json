{
  "name": "orderform",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "_moduleAliases": {
    "@root"      : ".",
    "@src"       : "src",
    "@controllers": "src/controllers",
    "@routes"    : "src/routes",
    "@models"    : "src/models",
    "@utils"     : "src/utils",
    "@public"    : "public",
    "@views"     : "views"
  },
  "scripts": {
    "build": "webpack",
    "nodemon": "nodemon ./bin/www",
    "tailwind:css": "postcss src/assets/styles/tailwind.css -o public/css/style.css --watch",
    "start": "node ./bin/www",
    "webpack:dev": "webpack --watch",
    "dev": "npm-run-all -p tailwind:css nodemon webpack:dev",
    "prisma:studio": "prisma studio",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@popperjs/core": "^2.11.8",
    "@preline/tooltip": "^2.5.0",
    "@prisma/client": "^5.20.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "babel-loader": "^9.2.1",
    "blob-stream": "^0.1.3",
    "browser-sync": "^3.0.2",
    "chalk": "^5.3.0",
    "connect-livereload": "^0.6.1",
    "connect-sqlite3": "^0.9.15",
    "cookie-parser": "~1.4.4",
    "css-loader": "^7.1.2",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-session": "^1.18.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "livereload": "^0.9.3",
    "mini-css-extract-plugin": "^2.9.2",
    "module-alias": "^2.2.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.5",
    "npm-run-all": "^4.1.5",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "preline": "^2.5.1",
    "prisma": "^5.22.0",
    "pug": "^3.0.3",
    "puppeteer": "^23.5.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.9",
    "@tailwindcss/forms": "^0.5.9",
    "@types/connect-livereload": "^0.6.3",
    "@types/connect-sqlite3": "^0.9.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/livereload": "^0.9.5",
    "@types/morgan": "^1.9.9",
    "@types/passport": "^1.0.17",
    "@types/passport-azure-ad": "^4.3.6",
    "@types/pug": "^2.0.10",
    "tailwindcss": "^3.4.14"
  }
